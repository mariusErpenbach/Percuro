<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:Percuro.ViewModels.EnterpriseViewModels.Analytics"
             x:Class="Percuro.Views.EnterpriseViews.Analytics.StatistikenView"
             x:DataType="vm:StatistikenViewModel">

    <Grid>
        <!-- To Analytics Button -->
        <Button Content="To Analytics" Command="{Binding ToAnalyticsView}" 
                Width="150" Height="50" Margin="10" 
                HorizontalAlignment="Left" VerticalAlignment="Top" 
                HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <TextBlock Text="This is the Statistiken View" HorizontalAlignment="Center" VerticalAlignment="Center" />
    </Grid>
</UserControl>